{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@nestx/nestx/(.*)": "<rootDir>/../libs/nestx/src/$1",
    "@nestx/nestx": "<rootDir>/../libs/nestx/src"
  }
}